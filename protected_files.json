  "important_files": {
    "description": "Files that are important for project structure and should be preserved unless explicitly requested",
    "files": [
      "update_metagraph_for_core.py",
      "verify_improvements.py",
      "test_strict_consensus.py",
      "test_metagraph_detailed.py",
