  "protected_directories": {
    "description": "Directories that contain important project structure and should be preserved",
    "directories": [
      "mt_core/",
      "subnet1_coreDAO/",
      "tests/",
      "scripts/",
      "examples/",
      "network/",
      "config/",
      "docker/",
      "docs/",
